{
  "name": "remix-authkit-example",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "reset": "rm -rf node_modules && rm -rf .cache && npm i",
    "start-local-dev": "rm -rf .cache && npm run prepare && npm run dev",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "prepare": "relative-deps",
    "prettier": "prettier \"{app,__tests__}/**/*.{js,ts,tsx}\" --check"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.0.5",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@workos-inc/authkit-remix": "0.7.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "relative-deps": "^1.0.7",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "relativeDependencies": {
    "@workos-inc/authkit-remix": "../authkit-remix"
  }
}
